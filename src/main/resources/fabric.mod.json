{
  "schemaVersion": 1,
  "id": "kinder",
  "version": "${version}",
  "name": "Kinder",
  "description": "A mod that aims to add Gems inspired by Steven Universe that aid the player in multiple ways, and even if you're not a fan of the show, the concepts are easy to understand.",
  "authors": [
    "Honeyed_Lemons_",
    "Segapop"
  ],
  "license": "CC0-1.0",
  "icon": "assets/kinder/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "honeyedlemons.kinder.init.KinderDataGenerator"
    ],
    "main": [
      "honeyedlemons.kinder.KinderMod"
    ],
    "client": [
      "honeyedlemons.kinder.KinderModClient"
    ],
    "modmenu": [
      "honeyedlemons.kinder.modcompat.KinderConfigMenu"
    ]
  },
  "mixins": [
    "kindergartening.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": ">=0.92.2",
    "geckolib": ">=4.4.6",
    "cloth-config": ">=11.1.118"
  },
  "accessWidener": "kindergartening.accesswidener"
}